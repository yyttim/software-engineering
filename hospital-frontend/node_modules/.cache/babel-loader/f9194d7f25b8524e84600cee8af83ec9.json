n                            round\n                            slot=\"append\"\n                            icon=\"iconfont icon-search-button\"\n                            @click=\"requestOrders\"\n                        ></el-button>\n                    </el-input>\n                </el-col>\n            </el-row>\n            <el-table size=\"small\" :data=\"orderData\" stripe style=\"width: 100%\" border>\n                <el-table-column prop=\"oId\" label=\"挂号单号\" width=\"80px\"/>\n                <el-table-column prop=\"dId\" label=\"本人id\" width=\"80px\"/>\n                <el-table-column prop=\"pId\" label=\"患者id\" width=\"80px\"/>\n                <el-table-column prop=\"oStart\" label=\"挂号时间\"width=\"190px\"/>\n                <el-table-column prop=\"oEnd\" label=\"结束时间\" width=\"150px\"/>\n                <el-table-column prop=\"oRecord\" label=\"病因\" width=\"200px\"/>\n                <el-table-column prop=\"oDrug\" label=\"药物\" width=\"180px\"/>\n                <el-table-column prop=\"oCheck\" label=\"检查项目\" width=\"180px\"/>\n                <el-table-column prop=\"oTotalPrice\" label=\"需交费用/元\" width=\"80px\"/>\n                <el-table-column prop=\"oPriceState\" label=\"缴费状态\" width=\"100px\"\n                >\n                    <template slot-scope=\"scope\">\n                        <el-tag\n                            type=\"success\"\n                            v-if=\"scope.row.oPriceState === 1\"\n                            >已缴费\n                        </el-tag>\n                        <el-tag\n                            type=\"danger\"\n                            v-if=\"\n                                scope.row.oPriceState === 0 &&\n                                scope.row.oState === 1\n                            \"\n                            >未缴费\n                        </el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"oState\" label=\"挂号状态\" width=\"100px\">\n                    <template slot-scope=\"scope\">\n                        <el-tag\n                            type=\"primary\"\n                            v-if=\"\n                                scope.row.oState === 1 &&\n                                scope.row.oPriceState === 1\n                            \"\n                            >已完成\n                        </el-tag>\n                        <el-tag\n                            type=\"danger\"\n                            v-if=\"\n                                scope.row.oState === 0 && scope.row.oState === 0\n                            \"\n                            >未完成\n                        </el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column label=\"操作\" width=\"140\" fixed=\"right\">\n                    <template slot-scope=\"scope\">\n                        <el-button\n                            size=\"mini\"\n                            type=\"warning\"\n                            icon=\"iconfont icon-diagnosis-button\"\n                            style=\"font-size: 14px\"\n                            @click=\"dealClick(scope.row.oId, scope.row.pId)\"\n                            v-if=\"\n                                scope.row.oState === 1 &&\n                                scope.row.oPriceState === 1\n                            \"\n                            >追诊</el-button\n                        >\n                    </template>\n                </el-table-column>\n            </el-table>\n\n            <!-- 分页 -->\n            <el-pagination\n                @size-change=\"handleSizeChange\"\n                @current-change=\"handleCurrentChange\"\n                background\n                layout=\"total, sizes, prev, pager, next, jumper\"\n                :current-page=\"pageNumber\"\n                :page-size=\"size\"\n                :page-sizes=\"[1, 2, 4, 8, 16]\"\n                :total=\"total\"\n            >\n            </el-pagination>\n        </el-card>\n    </div>\n</template>\n<script>\nimport request from \"@/utils/request.js\";\nimport jwtDecode from \"jwt-decode\";\nimport { getToken } from \"@/utils/storage.js\";\nexport default {\n    name: \"DoctorOrder\",\n    data() {\n        return {\n            userId: 1,\n            orderData: [],\n            pageNumber: 1,\n            size: 8,\n            query: \"\",\n            total: 3,\n        };\n    },\n    methods: {\n        //点击追诊按钮\n        dealClick(oId, pId) {\n            this.$router.push({\n                path: \"/dealOrderAgain\",\n                query: {\n                    oId: oId,\n                    pId: pId,\n                },\n            });\n        },\n        //页面大小改变时触发\n        handleSizeChange(size) {\n            this.size = size;\n            this.requestOrders();\n        },\n        //页码改变时触发\n        handleCurrentChange(num) {\n            this.pageNumber = num;\n            this.requestOrders();\n        },\n\n        //请求挂号信息\n        requestOrders() {\n            request\n                .get(\"order/findOrderByDid\", {\n                    params: {\n                        dId: this.userId,\n                        pageNumber: this.pageNumber,\n                        size: this.size,\n                        query: this.query,\n                    },\n                })\n                .then((res) => {\n                    if (res.data.status !== 200)\n                        this.$message.error(\"请求数据失败\");\n                    this.orderData = res.data.data.orders;\n                    this.total = res.data.data.total;\n                });\n        },\n        //token解码\n        tokenDecode(token) {\n            if (token !== null) return jwtDecode(token);\n        },\n    },\n    created() {\n        // 解码token\n        this.userId = this.tokenDecode(getToken()).dId;\n        this.requestOrders();\n    },\n};\n</script>\n<style lang=\"scss\" scoped>\n.el-table {\n    margin-top: 20px;\n    margin-bottom: 20px;\n}\n</style>\n"],"mappings":"AAuGA,OAAAA,OAAA;AACA,OAAAC,SAAA;AACA,SAAAC,QAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,SAAA;MACAC,UAAA;MACAC,IAAA;MACAC,KAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACA;IACAC,UAAAC,GAAA,EAAAC,GAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAC,IAAA;QACAR,KAAA;UACAI,GAAA,EAAAA,GAAA;UACAC,GAAA,EAAAA;QACA;MACA;IACA;IACA;IACAI,iBAAAV,IAAA;MACA,KAAAA,IAAA,GAAAA,IAAA;MACA,KAAAW,aAAA;IACA;IACA;IACAC,oBAAAC,GAAA;MACA,KAAAd,UAAA,GAAAc,GAAA;MACA,KAAAF,aAAA;IACA;IAEA;IACAA,cAAA;MACAnB,OAAA,CACAsB,GAAA;QACAC,MAAA;UACAC,GAAA,OAAAnB,MAAA;UACAE,UAAA,OAAAA,UAAA;UACAC,IAAA,OAAAA,IAAA;UACAC,KAAA,OAAAA;QACA;MACA,GACAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAtB,IAAA,CAAAuB,MAAA,UACA,KAAAC,QAAA,CAAAC,KAAA;QACA,KAAAvB,SAAA,GAAAoB,GAAA,CAAAtB,IAAA,CAAAA,IAAA,CAAA0B,MAAA;QACA,KAAApB,KAAA,GAAAgB,GAAA,CAAAtB,IAAA,CAAAA,IAAA,CAAAM,KAAA;MACA;IACA;IACA;IACAqB,YAAAC,KAAA;MACA,IAAAA,KAAA,kBAAA/B,SAAA,CAAA+B,KAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,KAAA5B,MAAA,QAAA0B,WAAA,CAAA7B,QAAA,IAAAsB,GAAA;IACA,KAAAL,aAAA;EACA;AACA","ignoreList":[]}]}