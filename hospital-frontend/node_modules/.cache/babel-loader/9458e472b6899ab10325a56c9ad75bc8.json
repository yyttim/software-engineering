uthor: ShanZhu\n * @Date: 2023-11-17\n-->\n<template>\n    <div>\n        <el-card>\n            <!-- 搜索栏 -->\n            <el-row type=\"flex\">\n                <el-col :span=\"8\">\n                    <el-input v-model=\"query\" placeholder=\"请输入患者id查询\">\n                        <el-button\n                            round\n                            slot=\"append\"\n                            icon=\"iconfont icon-search-button\"\n                            @click=\"requestBeds\"\n                        ></el-button>\n                    </el-input>\n                </el-col>\n                <el-col :span=\"1\"></el-col>\n            </el-row>\n\n          <!--添加病床按钮-->\n          <el-row type=\"flex\">\n                <el-col :span=\"6\">\n                    <br/>\n                    <el-button\n                        size=\"small\"\n                        type=\"success\"\n                        style=\"font-size: 15px\"\n                        @click=\"addFormVisible = true\"\n                    >\n                    <i class=\"iconfont icon-add-button\" style=\"font-size: 15px;\"></i>\n                        增加床位\n                    </el-button >\n                </el-col>\n            </el-row>\n\n            <!-- 表格 -->\n            <el-table :data=\"bedData\" size=\"small\" stripe style=\"width: 100%\" border>\n                <el-table-column label=\"床号\" prop=\"bId\"/>\n                <el-table-column label=\"患者id\" prop=\"pId\"/>\n                <el-table-column label=\"医生id\" prop=\"dId\"/>\n                <el-table-column label=\"开始时间\" prop=\"bStart\"/>\n                <el-table-column label=\"申请理由\"prop=\"bReason\"/>\n                <el-table-column label=\"状态\" prop=\"bState\">\n                    <template slot-scope=\"scope\">\n                        <el-tag v-if=\"scope.row.bState === 1\" type=\"danger\">\n                          已占用\n                        </el-tag>\n                        <el-tag v-if=\"scope.row.bState === 0\" type=\"primary\">\n                          空\n                        </el-tag>\n                    </template>\n                </el-table-column>\n                <el-table-column label=\"操作\" width=\"200\" fixed=\"right\">\n                    <template slot-scope=\"scope\">\n\n                        <el-button\n                            size=\"mini\"\n                            style=\"font-size: 14px\"\n                            type=\"danger\"\n                            @click=\"emptyDialog(scope.row.bId)\"\n                        ><i class=\"iconfont icon-delete-button\" style=\"font-size: 12px;\"></i></el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n\n            <!-- 分页 -->\n            <el-pagination\n                @size-change=\"handleSizeChange\"\n                @current-change=\"handleCurrentChange\"\n                background\n                layout=\"total, sizes, prev, pager, next, jumper\"\n                :current-page=\"pageNumber\"\n                :page-size=\"size\"\n                :page-sizes=\"[1, 2, 4, 8, 16]\"\n                :total=\"total\"\n            >\n            </el-pagination>\n        </el-card>\n\n        <!-- 增加床位对话框 -->\n        <el-dialog title=\"增加床位\" :visible.sync=\"addFormVisible\">\n            <el-form :model=\"addForm\" :rules=\"rules\" ref=\"ruleForm\">\n                <el-form-item label=\"床号\" prop=\"bId\" label-width=\"80px\">\n                    <el-input v-model.number=\"addForm.bId\"></el-input>\n                </el-form-item>\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"addFormVisible = false\" style=\"font-size: 18px;\"><i class=\"iconfont icon-r-left\" style=\"font-size: 20px;\"></i> 取 消</el-button>\n                <el-button type=\"primary\" @click=\"addBed('ruleForm')\"\n                    style=\"font-size: 18px;\"><i class=\"iconfont icon-r-yes\" style=\"font-size: 20px;\"></i> 确 定</el-button\n                >\n            </div>\n        </el-dialog>\n    </div>\n</template>\n<script>\nimport request from \"@/utils/request.js\";\nexport default {\n    name: \"CheckList\",\n    data() {\n        return {\n            pageNumber: 1,\n            size: 8,\n            query: \"\",\n            bedData: [],\n            total: 3,\n            addFormVisible: false,\n            addForm: {},\n            rules: {\n                bId: [\n                    { required: true, message: \"请输入床号\", trigger: \"blur\" },\n                    {\n                        type: \"number\",\n                        message: \"床号必须数字类型\",\n                        trigger: \"blur\",\n                    },\n                ],\n            },\n        };\n    },\n    methods: {\n        //清空床位操作\n        emptyBed(id) {\n            request\n                .get(\"bed/emptyBed\", {\n                    params: {\n                        bId: id,\n                    },\n                })\n                .then((res) => {\n                    this.requestBeds();\n                });\n        },\n        //清空对话框\n        emptyDialog(id) {\n            this.$confirm(\"此操作将清空该床位, 是否继续?\", \"提示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    this.emptyBed(id);\n                    this.$message({\n                        type: \"success\",\n                        message: \"清空成功!\",\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消清空\",\n                    });\n                });\n        },\n\n        //删除床位操作\n        deleteBed(id) {\n            request\n                .get(\"bed/deleteBed\", {\n                    params: {\n                        bId: id,\n                    },\n                })\n                .then((res) => {\n                    this.requestBeds();\n                });\n        },\n        //删除对话框\n        deleteDialog(id) {\n            this.$confirm(\"此操作将删除该床位, 是否继续?\", \"提示\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\",\n            })\n                .then(() => {\n                    this.deleteBed(id);\n                    this.$message({\n                        type: \"success\",\n                        message: \"删除成功!\",\n                    });\n                })\n                .catch(() => {\n                    this.$message({\n                        type: \"info\",\n                        message: \"已取消删除\",\n                    });\n                });\n        },\n        //点击增加确认按钮\n        addBed(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    request\n                        .get(\"bed/addBed\", {\n                            params: {\n                                bId: this.addForm.bId,\n                                pId: -1,\n                                dId: -1,\n                            },\n                        })\n                        .then((res) => {\n                            if (res.data.status !== 200)\n                                return this.$message.error(\"床号或已被占用！\");\n                            this.addFormVisible = false;\n                            this.requestBeds();\n                            this.$message.success(\"增加床位成功！\");\n                        });\n                } else {\n                    return false;\n                }\n            });\n        },\n        //页面大小改变时触发\n        handleSizeChange(size) {\n            this.size = size;\n            this.requestBeds();\n        },\n        //   页码改变时触发\n        handleCurrentChange(num) {\n            this.pageNumber = num;\n            this.requestBeds();\n        },\n        // 加载检查列表\n        requestBeds() {\n            request\n                .get(\"bed/findAllBeds\", {\n                    params: {\n                        pageNumber: this.pageNumber,\n                        size: this.size,\n                        query: this.query,\n                    },\n                })\n                .then((res) => {\n                    this.bedData = res.data.data.beds;\n                    this.total = res.data.data.total;\n                });\n        },\n    },\n    created() {\n        this.requestBeds();\n    },\n};\n</script>\n<style scoped lang=\"scss\">\n.el-table {\n    margin-top: 20px;\n    margin-bottom: 20px;\n}\n.el-form {\n    margin-top: 0;\n}\n</style>\n"],"mappings":"AAqGA,OAAAA,OAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,IAAA;MACAC,KAAA;MACAC,OAAA;MACAC,KAAA;MACAC,cAAA;MACAC,OAAA;MACAC,KAAA;QACAC,GAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UACAC,IAAA;UACAF,OAAA;UACAC,OAAA;QACA;MAEA;IACA;EACA;EACAE,OAAA;IACA;IACAC,SAAAC,EAAA;MACAlB,OAAA,CACAmB,GAAA;QACAC,MAAA;UACAT,GAAA,EAAAO;QACA;MACA,GACAG,IAAA,CAAAC,GAAA;QACA,KAAAC,WAAA;MACA;IACA;IACA;IACAC,YAAAN,EAAA;MACA,KAAAO,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAZ,IAAA;MACA,GACAM,IAAA;QACA,KAAAJ,QAAA,CAAAC,EAAA;QACA,KAAAU,QAAA;UACAb,IAAA;UACAF,OAAA;QACA;MACA,GACAgB,KAAA;QACA,KAAAD,QAAA;UACAb,IAAA;UACAF,OAAA;QACA;MACA;IACA;IAEA;IACAiB,UAAAZ,EAAA;MACAlB,OAAA,CACAmB,GAAA;QACAC,MAAA;UACAT,GAAA,EAAAO;QACA;MACA,GACAG,IAAA,CAAAC,GAAA;QACA,KAAAC,WAAA;MACA;IACA;IACA;IACAQ,aAAAb,EAAA;MACA,KAAAO,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAZ,IAAA;MACA,GACAM,IAAA;QACA,KAAAS,SAAA,CAAAZ,EAAA;QACA,KAAAU,QAAA;UACAb,IAAA;UACAF,OAAA;QACA;MACA,GACAgB,KAAA;QACA,KAAAD,QAAA;UACAb,IAAA;UACAF,OAAA;QACA;MACA;IACA;IACA;IACAmB,OAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACApC,OAAA,CACAmB,GAAA;YACAC,MAAA;cACAT,GAAA,OAAAF,OAAA,CAAAE,GAAA;cACA0B,GAAA;cACAC,GAAA;YACA;UACA,GA